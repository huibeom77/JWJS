/* #002_반복문_1924번 채희범 */
#include <iostream>
const unsigned int MAX = 12;
using namespace std;
void Calc(int Month, int Day) {
	int monthArr[MAX] = { 31, 28, 31, 30, 31, 30, 31, 31, 30, 31, 30, 31 };
	int sum = 0;
	for (int i = 0; i < Month; i++) {
		if (Day > monthArr[Month]) {
			cout << "월 최대 일수를 초과" << endl;
			return;
		}
		
		if (i == Month - 1) {
			sum += Day;
		}
		else {
			sum += monthArr[i];
		}
	}
	switch (sum%7)
	{
	case 1:
		cout << "MON" << endl;
		break;
	case 2:
		cout << "TUE" << endl;
		break;
	case 3:
		cout << "WED" << endl;
		break;
	case 4:
		cout << "THU" << endl;
		break;
	case 5:
		cout << "FRI" << endl;
		break;
	case 6:
		cout << "SAT" << endl;
		break;
	case 0:
		cout << "SUN" << endl;
		break;
	default:
		break;
	}
}

void run(void) {
	unsigned int getMonth, getDay;
	cin >> getMonth >> getDay;
	Calc(getMonth, getDay);

}

int main()
{
	run();
}
